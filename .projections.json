{
    ".config/nvim/init.vim": {
        "type": "vim",
        "_start": "vim"
    },
    ".config/nvim/ftplugin/*.vim": {
        "type": "ftplugin"
    },
    ".config/nvim/plugin/*.vim": {
        "type": "plugin"
    },
    ".config/nvim/bundle/": {
        "type": "vundles"
    },
    ".asoundrc": {
        "type": "asoundrc",
        "start": "speaker-test"
    },
    ".projections.json": {
        "type": "projections"
    },
    ".profile.d/*": {
        "type": "profile",
        "alternate": ".rc"
    },
    ".profile": {
        "type": "profile"
    }
}
